{
    "name": "m10c/content-elements-bundle",
    "description": "A Symfony Bundle that adds CMS functionality with Identity/Variant patterns, Dimensions, and Filters",
    "type": "symfony-bundle",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "M10c\\ContentElements\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M10c\\ContentElements\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.3",
        "api-platform/core": "^4.0",
        "doctrine/orm": "^2.12 || ^3.0",
        "symfony/cache": "^6.4 || ^7.0",
        "symfony/clock": "^6.4 || ^7.0",
        "symfony/dependency-injection": "^6.4 || ^7.0",
        "symfony/http-foundation": "^6.4 || ^7.0",
        "symfony/http-kernel": "^6.4 || ^7.0",
        "symfony/serializer": "^6.4 || ^7.0",
        "symfony/validator": "^6.4 || ^7.0",
        "webmozart/assert": "^1.11"
    },
    "require-dev": {
        "doctrine/common": "^3.5",
        "doctrine/doctrine-bundle": "^3.2",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-doctrine": "^2.0",
        "phpstan/phpstan-symfony": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^11.0",
        "symfony/browser-kit": "^6.4 || ^7.0",
        "symfony/framework-bundle": "^6.4 || ^7.0",
        "symfony/http-client": "^6.4 || ^7.0",
        "symfony/object-mapper": "^7.0",
        "symfony/security-bundle": "^6.4 || ^7.0",
        "symfony/yaml": "^6.4 || ^7.0"
    },
    "suggest": {
        "symfony/object-mapper": "Required for Map attributes in traits"
    },
    "scripts": {
        "phpstan": "vendor/bin/phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    }
}
